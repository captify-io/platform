{
  "slug": "agent",
  "name": "Agent Studio",
  "version": "1.0.0",
  "description": "AI agent builder and management platform",
  "menu": [
    {
      "label": "Agents",
      "href": "/agent",
      "icon": "Bot",
      "description": "Manage AI agents"
    },
    {
      "label": "Builder",
      "href": "/agent/builder",
      "icon": "Wrench",
      "description": "Build and configure agents"
    },
    {
      "label": "Workflows",
      "href": "/fabric/flow",
      "icon": "GitBranch",
      "description": "Agent workflows and automation"
    },
    {
      "label": "Providers",
      "href": "/agent/providers",
      "icon": "Cloud",
      "description": "AI model providers and settings"
    }
  ],
  "manifest": {
    "name": "Agent Studio",
    "icon": "Bot",
    "color": "#8b5cf6",
    "category": "ai",
    "tags": ["ai", "agents", "automation", "bedrock"],
    "visibility": "public",
    "resources": {
      "s3Bucket": "captify-agents"
    }
  },
  "features": [
    {
      "id": "agent-builder",
      "name": "Agent Builder",
      "description": "Visual agent configuration and building",
      "enabled": true,
      "icon": "Wrench",
      "href": "/agent/builder"
    },
    {
      "id": "workflows",
      "name": "Workflows",
      "description": "Create and manage agent workflows",
      "enabled": true,
      "icon": "GitBranch",
      "href": "/fabric/flow"
    },
    {
      "id": "providers",
      "name": "Provider Management",
      "description": "Configure AI model providers",
      "enabled": true,
      "icon": "Cloud",
      "href": "/agent/providers"
    }
  ],
  "access": {
    "requiresApproval": false,
    "defaultRole": "user",
    "allowedRoles": ["user", "developer", "admin"]
  }

{
  "id": "mi-app-001",
  "slug": "mi",
  "name": "Material Insights",
  "title": "BOM360 - Advanced Forecast & Decision Intelligence",
  "description": "150% Bill of Materials with predictive analytics for preventing MICAP situations",
  "version": "1.0.0",
  "category": "operations",
  "agentId": "MI_FORECAST_AGENT",
  "agentAliasId": "PROD",
  "database": {
    "tables": {
      "graph": {
        "tableName": "mi-bom-graph",
        "schema": {
          "partitionKey": "pk",
          "sortKey": "sk",
          "attributes": {
            "pk": {
              "type": "S",
              "description": "Partition key: NODE#, TAIL#, SUPPLIER#, ISSUE#, etc."
            },
            "sk": {
              "type": "S",
              "description": "Sort key: META, EDGE#, ALT#, TASK#, etc."
            },
            "type": {
              "type": "S",
              "description": "Entity type: META, EDGE, ALT, SUPERSESSION, etc."
            },
            "entity": {
              "type": "S",
              "description": "Entity category: Part, Assembly, System, Aircraft"
            },
            "name": { "type": "S", "description": "Human-readable name" },
            "wbs": {
              "type": "S",
              "description": "Work Breakdown Structure code"
            },
            "level": { "type": "N", "description": "BOM hierarchy level" },
            "attrs": {
              "type": "M",
              "description": "Extended attributes as JSON"
            },
            "version": { "type": "S", "description": "BOM version identifier" },
            "valid_from": {
              "type": "S",
              "description": "Effective start date"
            },
            "valid_to": { "type": "S", "description": "Effective end date" },
            "is_current": {
              "type": "BOOL",
              "description": "Current version flag"
            },
            "hash": {
              "type": "S",
              "description": "Content hash for integrity"
            },
            "gsi1pk": {
              "type": "S",
              "description": "GSI1 partition key for reverse queries"
            },
            "gsi1sk": {
              "type": "S",
              "description": "GSI1 sort key for reverse queries"
            },
            "gsi2pk": {
              "type": "S",
              "description": "GSI2 partition key for tail/effectivity"
            },
            "gsi2sk": {
              "type": "S",
              "description": "GSI2 sort key for tail/effectivity"
            },
            "gsi3pk": {
              "type": "S",
              "description": "GSI3 partition key for supplier queries"
            },
            "gsi3sk": {
              "type": "S",
              "description": "GSI3 sort key for supplier queries"
            },
            "gsi4pk": {
              "type": "S",
              "description": "GSI4 partition key for workbench ops"
            },
            "gsi4sk": {
              "type": "S",
              "description": "GSI4 sort key for workbench ops"
            },
            "created_at": { "type": "S", "description": "Creation timestamp" },
            "updated_at": {
              "type": "S",
              "description": "Last update timestamp"
            }
          },
          "globalSecondaryIndexes": [
            {
              "name": "GSI1",
              "partitionKey": "gsi1pk",
              "sortKey": "gsi1sk",
              "description": "Reverse relationships and alternate groups"
            },
            {
              "name": "GSI2",
              "partitionKey": "gsi2pk",
              "sortKey": "gsi2sk",
              "description": "Tail and effectivity queries"
            },
            {
              "name": "GSI3",
              "partitionKey": "gsi3pk",
              "sortKey": "gsi3sk",
              "description": "Supplier-centric views"
            },
            {
              "name": "GSI4",
              "partitionKey": "gsi4pk",
              "sortKey": "gsi4sk",
              "description": "Workbench operations and task management"
            }
          ]
        }
      }
    }
  },
  "menu": [
    {
      "id": "advanced-forecast",
      "label": "Advanced Forecast",
      "icon": "TrendingUp",
      "path": "/mi/advanced-forecast",
      "description": "Predictive analytics and MICAP prevention",
      "isPrimary": true
    },
    {
      "id": "bom-explorer",
      "label": "BOM Explorer",
      "icon": "Network",
      "path": "/mi/bom-explorer",
      "description": "150% BOM navigation with configurations & variants"
    },
    {
      "id": "workbench",
      "label": "Workbench",
      "icon": "Wrench",
      "path": "/mi/workbench",
      "description": "Collaborative problem resolution workflow"
    },
    {
      "id": "supply-chain",
      "label": "Supply Chain Insights",
      "icon": "Truck",
      "path": "/mi/supply-chain",
      "description": "Supplier health and part availability"
    },
    {
      "id": "analytics",
      "label": "Analytics & Reports",
      "icon": "BarChart3",
      "path": "/mi/analytics",
      "description": "Reliability analysis and readiness reporting"
    },
    {
      "id": "documents",
      "label": "Document Library",
      "icon": "FileText",
      "path": "/mi/documents",
      "description": "Technical orders and linked documentation"
    },
    {
      "id": "settings",
      "label": "App Settings",
      "icon": "Settings",
      "path": "/mi/settings",
      "description": "Application configuration and preferences"
    }
  ],
  "features": {
    "forecast": {
      "enabled": true,
      "models": ["v1.3", "v1.2", "v1.1"],
      "defaultModel": "v1.3",
      "windows": [30, 60, 90],
      "defaultWindow": 30,
      "refreshInterval": 300000
    },
    "bomExplorer": {
      "enabled": true,
      "maxDepth": 5,
      "defaultDepth": 3,
      "views": [
        "Engineering",
        "Manufacturing",
        "Supply Chain",
        "Sustainment",
        "SPO"
      ],
      "defaultView": "Engineering"
    },
    "workbench": {
      "enabled": true,
      "stages": [
        "Intake",
        "Analyze",
        "Validate Solution",
        "Qualify",
        "Field",
        "Monitor"
      ],
      "priorities": ["Low", "Medium", "High", "Critical"],
      "aiRecommendations": true
    },
    "supplyChain": {
      "enabled": true,
      "metrics": ["otd", "leadTime", "quality", "cost"],
      "alertThresholds": {
        "otd": 0.85,
        "leadTime": 45,
        "quality": 0.95
      }
    },
    "realTimeUpdates": {
      "enabled": true,
      "updateInterval": 30000,
      "channels": ["forecast", "workbench", "supplier"]
    }
  },
  "demo": {
    "enabled": true,
    "scenario": "b52h-crisis",
    "aircraft": "B-52H",
    "tail": "60-0020",
    "criticalComponent": {
      "id": "nsn:2840-00-123-4567",
      "name": "Combustion Module",
      "riskScore": 0.86,
      "daysToFailure": 14,
      "missionImpact": 2100000
    },
    "suppliers": [
      {
        "cage": "1AB23",
        "name": "Primary Aerospace Corp",
        "leadTime": 62,
        "otd": 0.82,
        "cost": 18420.0,
        "status": "problematic"
      },
      {
        "cage": "7XY89",
        "name": "Alternate Manufacturing Inc",
        "leadTime": 18,
        "otd": 0.94,
        "cost": 21183.0,
        "status": "recommended"
      },
      {
        "cage": "2CD45",
        "name": "Reliable Parts LLC",
        "leadTime": 35,
        "otd": 0.91,
        "cost": 18850.0,
        "status": "backup"
      }
    ]
  },
  "streams": {
    "forecast": {
      "endpoint": "/api/mi/stream/forecast",
      "cacheDuration": 300,
      "parameters": ["scope", "id", "window", "model", "asof"]
    },
    "bom": {
      "endpoint": "/api/mi/stream/bom",
      "cacheDuration": 600,
      "parameters": ["nodeId", "depth", "view", "asof"]
    },
    "workbench": {
      "endpoint": "/api/mi/stream/workbench",
      "cacheDuration": 60,
      "parameters": ["status", "priority", "assignee"]
    },
    "supplier": {
      "endpoint": "/api/mi/stream/supplier",
      "cacheDuration": 300,
      "parameters": ["supplierId", "metrics", "asof"]
    }
  },
  "integrations": {
    "bedrock": {
      "enabled": true,
      "agentId": "MI_FORECAST_AGENT",
      "aliasId": "PROD",
      "region": "us-east-1",
      "capabilities": [
        "risk_analysis",
        "supplier_recommendations",
        "cost_optimization",
        "decision_support"
      ]
    },
    "s3": {
      "enabled": true,
      "bucket": "mi-technical-orders",
      "documentTypes": ["pdf", "docx", "dwg"],
      "maxFileSize": 50000000
    },
    "eventbridge": {
      "enabled": true,
      "events": [
        "forecast.updated",
        "issue.created",
        "issue.resolved",
        "supplier.alert"
      ]
    }
  },
  "ui": {
    "theme": {
      "primary": "#0ea5e9",
      "secondary": "#64748b",
      "accent": "#f59e0b",
      "success": "#10b981",
      "warning": "#f59e0b",
      "error": "#ef4444",
      "critical": "#dc2626"
    },
    "animations": {
      "enabled": true,
      "duration": 200,
      "easing": "ease-in-out"
    },
    "charts": {
      "library": "recharts",
      "defaultColors": ["#0ea5e9", "#64748b", "#f59e0b", "#10b981", "#ef4444"],
      "responsive": true
    }
  },
  "security": {
    "dataClassification": "CUI",
    "auditLogging": true,
    "encryptionAtRest": true,
    "accessControl": {
      "organizationIsolation": true,
      "roleBasedAccess": true,
      "dataFiltering": true
    }
  },
  "performance": {
    "caching": {
      "enabled": true,
      "provider": "redis",
      "ttl": 300
    },
    "pagination": {
      "defaultPageSize": 50,
      "maxPageSize": 1000
    },
    "virtualization": {
      "enabled": true,
      "threshold": 100
    }
  }
}

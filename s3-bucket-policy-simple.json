{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowMiAppUserFullAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::211125459951:user/mi-app-user"
      },
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951",
        "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951/*"
      ]
    },
    {
      "Sid": "AllowElasticBeanstalkServiceAccess",
      "Effect": "Allow",
      "Principal": {
        "Service": "elasticbeanstalk.amazonaws.com"
      },
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:GetObjectAcl",
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:DeleteObject",
        "s3:DeleteObjectVersion"
      ],
      "Resource": "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951/*"
    },
    {
      "Sid": "AllowElasticBeanstalkListBucket",
      "Effect": "Allow",
      "Principal": {
        "Service": "elasticbeanstalk.amazonaws.com"
      },
      "Action": [
        "s3:ListBucket",
        "s3:ListBucketVersions",
        "s3:GetBucketLocation",
        "s3:GetBucketVersioning"
      ],
      "Resource": "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951"
    },
    {
      "Sid": "AllowAccountRootFullControl",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::211125459951:root"
      },
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951",
        "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951/*"
      ]
    }
  ]
}
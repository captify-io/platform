{
  "name": "@captify/api",
  "version": "0.0.1",
  "description": "API client with built-in security for Captify SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "watch": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@captify/core": "workspace:*",
    "@aws-sdk/client-bedrock-agent": "^3.848.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.857.0",
    "@aws-sdk/client-bedrock-runtime": "^3.857.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.848.0",
    "@aws-sdk/client-dynamodb": "^3.864.0",
    "@aws-sdk/client-lambda": "^3.859.0",
    "@aws-sdk/client-neptunedata": "^3.851.0",
    "@aws-sdk/client-opensearchserverless": "^3.859.0",
    "@aws-sdk/client-organizations": "^3.848.0",
    "@aws-sdk/client-service-catalog": "^3.848.0",
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/client-ssm": "^3.849.0",
    "@aws-sdk/client-sts": "^3.848.0",
    "@aws-sdk/credential-providers": "^3.857.0",
    "@aws-sdk/lib-dynamodb": "^3.859.0",
    "@aws-sdk/types": "^3.840.0",
    "@aws-sdk/util-dynamodb": "^3.864.0",
    "aws-sdk": "^2.1692.0",
    "next-auth": "^4.24.11"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}

{
  "name": "@captify/api",
  "version": "0.0.1",
  "description": "API client with built-in security for Captify SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent": "^3.873.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.873.0",
    "@aws-sdk/client-bedrock-runtime": "^3.873.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.873.0",
    "@aws-sdk/client-dynamodb": "^3.873.0",
    "@aws-sdk/client-lambda": "^3.873.0",
    "@aws-sdk/client-neptunedata": "^3.873.0",
    "@aws-sdk/client-opensearchserverless": "^3.873.0",
    "@aws-sdk/client-organizations": "^3.873.0",
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/client-service-catalog": "^3.873.0",
    "@aws-sdk/client-ssm": "^3.873.0",
    "@aws-sdk/client-sts": "^3.873.0",
    "@aws-sdk/credential-providers": "^3.873.0",
    "@aws-sdk/lib-dynamodb": "^3.873.0",
    "@aws-sdk/types": "^3.862.0",
    "@aws-sdk/util-dynamodb": "^3.873.0",
    "next-auth": "^4.24.11"
  },
  "peerDependencies": {
    "next": "^15.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}

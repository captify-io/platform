{
  "name": "@captify/mi",
  "version": "1.0.0",
  "description": "Materiel Insights - Aircraft Lifecycle Management",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.18 || >=20"
  },
  "main": "dist/app.js",
  "module": "dist/app.js",
  "types": "dist/app.d.ts",
  "exports": {
    ".": {
      "types": "./dist/app.d.ts",
      "default": "./dist/app.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "default": "./dist/app.js"
    },
    "./services": {
      "types": "./dist/services.d.ts",
      "default": "./dist/services.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "default": "./dist/components.js"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "default": "./dist/context.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "default": "./dist/hooks.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "default": "./dist/api.js"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "default": "./dist/ui.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "@captify/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tsup": "^8.0.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": ">=19.1.0",
    "react-dom": ">=19.1.0"
  },
  "keywords": [
    "captify",
    "aircraft",
    "plm",
    "materiel",
    "lifecycle",
    "maintenance",
    "bom"
  ]
}

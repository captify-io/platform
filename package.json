    "glob": "^10.0.0",
    "cross-spawn-async": "cross-spawn"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.2",
    "@types/node": "^24.3.1",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "tsup": "^8.5.0"
  }
}

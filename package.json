{
  "name": "captify-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:watch": "concurrently \"pnpm run dev:packages\" \"pnpm run dev\" --names \"ðŸ“¦,âš¡\" --prefix-colors \"blue,green\"",
    "dev:packages": "pnpm --filter @captify/core run dev",
    "build": "next build",
    "build:debug": "cross-env NODE_OPTIONS=--trace-warnings next build",
    "build:amplify": "pnpm run build:packages && next build",
    "build:packages": "pnpm --filter @captify/core run build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "type-check:packages": "pnpm --filter @captify/core run type-check",
    "deploy:check": "pnpm run type-check && pnpm run type-check:packages && pnpm run lint",
    "clean": "rm -rf .next dist && pnpm --filter @captify/core run clean",
    "clean:all": "pnpm run clean && rm -rf node_modules packages/*/node_modules",
    "install:list": "node install.js list",
    "install:package": "node install.js install",
    "install:core": "node install.js install core",
    "install:core-metadata": "node install.js install core --TableMetadata"
  },

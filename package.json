{
  "name": "captify-workspace",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:all": "turbo run dev",
    "dev:packages": "turbo run dev --filter=packages/*",
    "build": "next build",
    "build:all": "turbo run build",
    "build:packages": "turbo run build --filter=packages/*",
    "start": "next start",
    "lint": "next lint",
    "lint:all": "turbo run lint",
    "lint:packages": "turbo run lint --filter=packages/*",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:all": "turbo run type-check",
    "type-check:packages": "turbo run type-check --filter=packages/*",
    "validate": "npm run lint && npm run type-check && npm run build",
    "validate:all": "turbo run lint type-check build",
    "validate:packages": "turbo run lint type-check build --filter=packages/*",
    "clean": "rm -rf .next out dist",
    "clean:all": "turbo run clean",
    "clean:packages": "turbo run clean --filter=packages/*",
    "dev:turbo": "next dev --turbopack",
    "dev:fast": "next dev --turbopack --port 3001",
    "build:analyze": "ANALYZE=true next build",
    "build:turbo": "next build --experimental-build-mode=compile",
    "deploy": "bash deploy.sh",
    "deploy:dev": "bash deploy.sh dev",
    "deploy:staging": "bash deploy.sh staging",
    "deploy:prod": "bash deploy.sh prod",
    "deploy:windows": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "deploy:windows:dev": "powershell -ExecutionPolicy Bypass -File deploy.ps1 dev",
    "deploy:windows:staging": "powershell -ExecutionPolicy Bypass -File deploy.ps1 staging",
    "deploy:windows:prod": "powershell -ExecutionPolicy Bypass -File deploy.ps1 prod",
    "sync-env": "bash sync-env.sh",
    "sync-env:windows": "powershell -ExecutionPolicy Bypass -File sync-env.ps1",
    "amplify:status": "amplify status",
    "amplify:console": "amplify console",
    "setup-local-db": "tsx scripts/setup-local-dynamodb.ts",
    "db:local": "docker run -p 8000:8000 amazon/dynamodb-local",
    "install-app": "node packages/install.js",
    "install-db": "tsx scripts/install.ts",
    "install-db:fast": "tsx scripts/install.ts --skip-wait"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.39",
    "@ai-sdk/azure": "^1.0.9",
    "@ai-sdk/google": "^1.0.7",
    "@ai-sdk/openai": "^0.0.46",
    "@ai-sdk/react": "^0.0.37",
    "@aws-sdk/client-bedrock-agent": "^3.848.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.857.0",
    "@aws-sdk/client-bedrock-runtime": "^3.857.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.848.0",
    "@aws-sdk/client-dynamodb": "^3.864.0",
    "@aws-sdk/client-lambda": "^3.859.0",
    "@captify/core": "workspace:*",
    "@captify/chat": "workspace:*",
    "@captify/api": "workspace:*",
    "@captify/veripicks": "workspace:*",
    "@aws-sdk/client-neptunedata": "^3.851.0",
    "@aws-sdk/client-opensearchserverless": "^3.859.0",
    "@aws-sdk/client-organizations": "^3.848.0",
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/client-service-catalog": "^3.848.0",
    "@aws-sdk/client-ssm": "^3.849.0",
    "@aws-sdk/client-sts": "^3.848.0",
    "@aws-sdk/credential-providers": "^3.857.0",
    "@aws-sdk/lib-dynamodb": "^3.859.0",
    "@aws-sdk/types": "^3.840.0",
    "@aws-sdk/util-dynamodb": "^3.864.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ai": "^4.2.0",
    "aws-sdk": "^2.1692.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "gremlin": "^3.7.3",
    "gremlin-aws-sigv4": "^3.6.1",
    "groq-sdk": "^0.29.0",
    "lucide-react": "^0.526.0",
    "next": "15.4.4",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^3.1.2",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/gremlin": "^3.6.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "tailwindcss": "^4",
    "tsup": "^8.0.0",
    "tsx": "^4.20.3",
    "turbo": "^2.5.6",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}

{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowMiAppUserFullAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::211125459951:user/mi-app-user"
      },
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951",
        "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951/*"
      ]
    },
    {
      "Sid": "AllowElasticBeanstalkServiceAccess",
      "Effect": "Allow",
      "Principal": {
        "Service": "elasticbeanstalk.amazonaws.com"
      },
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:DeleteObject",
        "s3:DeleteObjectVersion",
        "s3:PutObject"
      ],
      "Resource": "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951/*"
    },
    {
      "Sid": "AllowElasticBeanstalkEC2Access",
      "Effect": "Allow",
      "Principal": {
        "AWS": [
          "arn:aws:iam::211125459951:role/aws-elasticbeanstalk-ec2-role",
          "arn:aws:iam::211125459951:role/aws-elasticbeanstalk-service-role"
        ]
      },
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:DeleteObject",
        "s3:DeleteObjectVersion"
      ],
      "Resource": "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951/*"
    },
    {
      "Sid": "AllowElasticBeanstalkListBucket",
      "Effect": "Allow",
      "Principal": {
        "Service": "elasticbeanstalk.amazonaws.com"
      },
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation",
        "s3:GetBucketVersioning"
      ],
      "Resource": "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951"
    },
    {
      "Sid": "AllowElasticBeanstalkEC2ListBucket",
      "Effect": "Allow",
      "Principal": {
        "AWS": [
          "arn:aws:iam::211125459951:role/aws-elasticbeanstalk-ec2-role",
          "arn:aws:iam::211125459951:role/aws-elasticbeanstalk-service-role"
        ]
      },
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951"
    },
    {
      "Sid": "PublicReadForEB",
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::elasticbeanstalk-us-east-1-211125459951/deployments/*",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "211125459951"
        }
      }
    }
  ]
}
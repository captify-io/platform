# AWS Credentials for Cognito Service
# These credentials need IAM permissions for Cognito admin operations
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# AWS Region (optional, defaults to us-east-1)
AWS_REGION=us-east-1

# Cognito User Pool ID
COGNITO_USER_POOL_ID=us-east-1_k3Fp77c09

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Session Configuration (optional)
NEXTAUTH_SESSION_DURATION=3600
COGNITO_TOKEN_REFRESH_BUFFER=300
{
  "envs": {
    "dev": {
      "buildCommand": "pnpm run build",
      "testCommand": "pnpm run lint && pnpm run type-check",
      "startCommand": "pnpm run start"
    },
    "staging": {
      "buildCommand": "pnpm run build",
      "testCommand": "pnpm run validate",
      "startCommand": "pnpm run start"
    },
    "prod": {
      "buildCommand": "pnpm run build",
      "testCommand": "pnpm run validate",
      "startCommand": "pnpm run start"
    }
  },
  "buildSettings": {
    "nodeVersion": "22.15.1",
    "packageManager": "pnpm",
    "artifactBaseDirectory": ".next",
    "cacheDirectories": [
      ".next/cache",
      "node_modules/.pnpm",
      "~/.pnpm-store"
    ]
  }
}
